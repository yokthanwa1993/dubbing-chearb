{
    "name": "dubbing-chearb-worker",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "wrangler dev",
        "deploy": "wrangler deploy",
        "db:create": "wrangler d1 create dubbing-chearb-db",
        "db:migrate": "wrangler d1 execute dubbing-chearb-db --file=./schema.sql",
        "db:migrate:local": "wrangler d1 execute dubbing-chearb-db --local --file=./schema.sql"
    },
    "dependencies": {
        "@cloudflare/containers": "^0.1.0",
        "hono": "^4.12.1"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20240117.0",
        "typescript": "^5.3.3",
        "wrangler": "^4.65.0"
    }
}
